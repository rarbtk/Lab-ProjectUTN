								Matriz[ID_DESBLOQUEAR,6] <- "1"
								ESTADO <- "Activo"
								Escribir "El usuario " Matriz[ID_DESBLOQUEAR,1] " " Matriz[ID_DESBLOQUEAR,2] " ahora esta " ESTADO
							Fin Si
						Fin si
					FinSi
				FinSi
				
			Hasta Que ID_DESBLOQUEAR < 1 Y ID_DESBLOQUEAR < LONGITUD_USUARIOS_REGISTRADOS
			
			
				
				//----------------------------------------------------------------------------
				//----------------------------------------------------------------------------
		4: 
			Limpiar Pantalla
			Para k<-1 Hasta 98 Con Paso 1 Hacer
				Si Matriz[k,1] == 'zxz' Entonces
					LONGITUD_USUARIOS_REGISTRADOS <- k
					k <- 98
				Fin Si
			Fin Para
			
