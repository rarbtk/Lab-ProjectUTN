//OPCIONES DE USUARIO ADMINISTADOR
SubProceso OPCIONES_ADMIN(Matriz,OpcUsuario,Dinero,Historial)
	
	//La variable ID_DESBLOQUEAR se usa para identificar el usuario a cambiar su estado
	DEFINIR i,j,k,Z, flag, ID_DESBLOQUEAR, LONGITUD_USUARIOS_REGISTRADOS, CAMBIO_ESTADO,OPCION_A_MODIFICAR, ID_HISTORIAL Como Entero
	// las variables longitud se usan para validar el largo
	DEFINIR LONGITUD_NOMBRE, LONGITUD_APELLIDO, LONGITUD_DNI, LONGITUD_CONTRASENA,mal_dato  Como Entero
	//DEFINIR OPCION_A_MODIFICAR Como Entero
	DEFINIR NUEVO_ID, NUEVO_NOMBRE, NUEVO_APELLIDO, NUEVO_DNI,NUEVA_CONTRASENA,ESTADO,CONFIRMAR_CONTRASENA, validador, CUENTA_PESOS_CARACTER Como Caracter
	Definir CUENTA_PESOS Como Real
	
	Segun OpcUsuario Hacer
		1:
			//Opción para listar usuario, listara los usuarios cuyo ID sea diferente a "zxz".
			Limpiar Pantalla
			Para i<-1 Hasta 98 Con Paso 1 Hacer
				SI Matriz[i,0] <> "zxz" Y Matriz[i,6] == "1" Entonces
					Escribir '----------------------------------------------------------------------------------------------'
					Escribir ' ..----.----.----.----.----.----.----.----.----.----.----.----.----.----.----.----.----.----..'
					Escribir 'ID: ' Matriz[i,0] ' Nombre completo: ' Matriz[i,1] ' ' Matriz[i,2] ' DNI: ' Matriz[i,3]  ' Cuenta Pesos: $' Dinero[i] ' Contrasena: ' Matriz[i,4]
					Escribir ' ..----.----.----.----.----.----.----.----.----.----.----.----.----.----.----.----.----.----..'
				FinSi
			Fin Para
		2:
			//OPCIÓN 2 PARA CREAR USUARIO
			Limpiar Pantalla
			Para i<-1 Hasta 98 Con Paso 1 Hacer
				SI Matriz[i,0] == "zxz" Entonces
					NUEVO_ID <- ConvertirATexto(i)
					Matriz[i,0] <- NUEVO_ID
					Limpiar Pantalla
					Escribir '                             'Sin Saltar
					Escribir ' -----------------------------------'Sin Saltar
					Escribir '                              '
					Escribir '                             'Sin Saltar
					Escribir '|<<<<<<----Agregar Usuario---->>>>>>|'Sin Saltar
					Escribir '                              '
					Escribir '                             'Sin Saltar
					Escribir ' -----------------------------------'
					Escribir '                              '
										
					Repetir
						Escribir '                              'Sin saltar
						Escribir '[1] Ingrese el Nombre: '
						Escribir '                             'Sin Saltar
						Leer NUEVO_NOMBRE
						LONGITUD_NOMBRE <- Longitud(NUEVO_NOMBRE)
						SI LONGITUD_NOMBRE >=3 Entonces
							Matriz[i,1] <- NUEVO_NOMBRE
						SINO
							Escribir '                              'Sin saltar
							Escribir "El nombre del usuario debe tener al menos 3 caracteres"	
						FinSi
						
						mal_dato <- 0
						Para Z<-0 Hasta LONGITUD_NOMBRE Con Paso 1 Hacer
							validador <- Subcadena(NUEVO_NOMBRE,Z,1)
							si validador == "0" o validador == "1" o validador == "2" o validador == "3" o validador == "4" o validador == "5" o validador == "6" o validador == "7" o validador == "8" o validador == "9" o validador == " " entonces
								mal_dato <-1
							FinSi
							
						Fin Para
						Si mal_dato == 1 Entonces
							Escribir '                              'Sin saltar
							Escribir "Los nombres no deben tener numeros ni espacios"	
						FinSi
						
					Hasta Que LONGITUD_NOMBRE > 3 Y mal_dato < 1

					Repetir
						Escribir '                              'Sin saltar
						Escribir '[2] Ingrese el Apellido: '
						Escribir '                             'Sin Saltar
						Leer NUEVO_APELLIDO
						LONGITUD_APELLIDO <- Longitud(NUEVO_APELLIDO)
						
						SI LONGITUD_APELLIDO >=3 Entonces
							Matriz[i,2] <- NUEVO_APELLIDO
						SINO
							Escribir '                              'Sin saltar
							Escribir "El apellido del usuario debe tener al menos 3 caracteres"	
						FinSi
						
						mal_dato <- 0
						Para Z<-0 Hasta LONGITUD_APELLIDO Con Paso 1 Hacer
							validador <- Subcadena(NUEVO_APELLIDO,Z,1)
							si validador == "0" o validador == "1" o validador == "2" o validador == "3" o validador == "4" o validador == "5" o validador == "6" o validador == "7" o validador == "8" o validador == "9" entonces
								mal_dato <-1
							FinSi
							
						Fin Para
						Si mal_dato == 1 Entonces
							Escribir '                              'Sin saltar
							Escribir "Los apellidos no deben tener numeros ni espacios"	
						FinSi
						
					Hasta Que LONGITUD_APELLIDO > 3 Y mal_dato < 1
					
					
					Matriz[i,2] <- NUEVO_APELLIDO
					
					Repetir
						Escribir '                              'Sin saltar
						Escribir '[3] Ingrese el DNI: '
						Escribir '                             'Sin Saltar
						Leer NUEVO_DNI
						LONGITUD_DNI <- Longitud(NUEVO_DNI)
						mal_dato <- 0

						SI LONGITUD_DNI > 5 y LONGITUD_DNI < 9 Entonces
							Para Z<-1 Hasta 98 Con Paso 1 Hacer
								si Matriz[Z,3] == NUEVO_DNI Entonces
									mal_dato <- 2
								FinSi
							Fin Para	
						SINO
							Escribir '                              'Sin saltar
							Escribir "El DNI del usuario debe mayor a 100 mil y menor a 100 millones"	
						FinSi
						
						Para Z<-0 Hasta LONGITUD_DNI Con Paso 1 Hacer
							validador <- Subcadena(NUEVO_DNI,Z,1)
							validador <- Minusculas(validador)
							si validador == "a" o validador == "b" o validador == "c" o validador == "d" o validador == "e" o validador == "f" o validador == "g" o validador == "h" o validador == "i" o validador == "j" o validador == "k" o validador == "l" o validador == "m" o validador == "n" o validador == "o" o validador == "p" o validador == "q" o validador == "r" o validador == "s" o validador == "t" o validador == "u" o validador == "v" o validador == "w" o validador == "x" o validador == "y" o validador == "z" entonces
								mal_dato <-3
							FinSi
						Fin Para
						Si mal_dato == 3 Entonces
							Escribir '                              'Sin saltar
							Escribir "Los DNI no deben tener letras ni espacios"	
						FinSi
						
						si mal_dato == 2 Entonces
							Escribir '                              'Sin saltar
							Escribir "El DNI ya existe, por favor ingrese otro"	
						FinSi
						
					Hasta Que LONGITUD_DNI > 5 Y mal_dato < 1 Y LONGITUD_DNI < 9
					Matriz[i,3] <- NUEVO_DNI
					
					Repetir
						mal_dato <- 0
						Escribir '                              'Sin saltar
						Escribir '[4] Ingrese la contraseña: '
						Escribir '                              'Sin saltar
						Leer NUEVA_CONTRASENA
						lONGITUD_CONTRASENA <- Longitud(NUEVA_CONTRASENA)
						
						SI lONGITUD_CONTRASENA < 4 Entonces
							Escribir '                              'Sin saltar
							Escribir "La contrasena debe tener minimo 4 caracteres"
							mal_dato <- 1
						FinSi
					Hasta Que mal_dato <1 Y lONGITUD_CONTRASENA >= 4
					
					Matriz[i,4] <- NUEVA_CONTRASENA
					
					Repetir
						mal_dato <-0
						Escribir '                              'Sin saltar
						Escribir '[5] Dinero a depositar: '
						Escribir '                              'Sin saltar
						Leer CUENTA_PESOS_CARACTER
						
						Para Z<-0 Hasta Longitud(CUENTA_PESOS_CARACTER) Con Paso 1 Hacer
							validador <- Subcadena(CUENTA_PESOS_CARACTER,Z,1)
							validador <- Minusculas(validador)
							si validador == "a" o validador == "b" o validador == "c" o validador == "d" o validador == "e" o validador == "f" o validador == "g" o validador == "h" o validador == "i" o validador == "j" o validador == "k" o validador == "l" o validador == "m" o validador == "n" o validador == "o" o validador == "p" o validador == "q" o validador == "r" o validador == "s" o validador == "t" o validador == "u" o validador == "v" o validador == "w" o validador == "x" o validador == "y" o validador == "z" entonces
								mal_dato <-3
							FinSi
						finpara
												
							si mal_dato < 1 Entonces
								CUENTA_PESOS <- ConvertirANumero(CUENTA_PESOS_CARACTER)
								Dinero[i] <- CUENTA_PESOS
							SiNo
								Escribir '                              'Sin saltar
								Escribir "Solo puedes ingresar numeros"
							finsi	
								
							si CUENTA_PESOS < 0 Entonces
								Escribir '                              'Sin saltar
								Escribir "Solo puedes ingresar numeros positivos"
								mal_dato <- 1
							FinSi

					Hasta Que mal_dato < 1
					
					
					
					Matriz[i,6] <- "1"
					
					Escribir '                              'Sin saltar
					Escribir '.--.----.----.----.----.----.----.--.'
					Escribir '                              'Sin saltar
					Escribir '(1) Para crear otro usuario o (2) para salir.'
					Escribir '                              'Sin saltar
					leer flag
					
					Si flag == 2 Entonces
						i<-98
					FinSi
				FinSi
			Fin Para
		3:
			//Con esta opciÓN podremos bloquear o desbloquear un usuario.
			//Este ciclo, mide cuántos usuarios registrados hay en el sistema, encontrando la primera coincidencia... 
			//...con "zxz" y almacenando en la variable "longitud_usuarios_registrado" el número.
			Limpiar Pantalla
			Para k<-1 Hasta 98 Con Paso 1 Hacer
					Si Matriz[k,1] == 'zxz' Entonces
						LONGITUD_USUARIOS_REGISTRADOS <- k
						k <- 98
					Fin Si
			Fin Para
			
			//Pedimos al usuario el id que quiere modificar.
			
			Repetir
				Escribir '-----------------------------------------------------------'
				Escribir ' ..----.----.----.----.----.----.----.----.----.----.----..'
				eSCRIBIR '         'Sin Saltar
				Escribir 'Seleccione el ID del usuario a desbloquear'
				Escribir ' ..----.----.----.----.----.----.----.----.----.----.----..'
				Escribir '-----------------------------------------------------------'
				Para i<-1 Hasta LONGITUD_USUARIOS_REGISTRADOS-1 Con Paso 1 Hacer				
					SI Matriz[i,6] == "1" Entonces
						ESTADO <- "Activo"
					SiNo
						ESTADO <-"Bloqueado"
					Fin Si
					Escribir '____________________________________________________________'
					Escribir '[ID]: ' i ' Nombre Apellido: ' Matriz[i,1] ' ' Matriz[i,2] ' Estado: ' ESTADO
					Escribir '____________________________________________________________'
				Fin Para
				Escribir '(0) Para cancelar.'
				Leer ID_DESBLOQUEAR
				
				SI ID_DESBLOQUEAR > 0 Entonces
					
					SI Matriz[ID_DESBLOQUEAR,6] == "1" Entonces
						ESTADO <- "Activo"
