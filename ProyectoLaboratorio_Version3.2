//FUNCION PARA TRANSFERENCIAS
SubProceso TRANSFERENCIAS(Matriz,Dinero,ID,Historial)
	
	Definir Trans_Dinero Como Real
	Definir Confir_Dinero,OpcIngreso,cont_trans,ID_Receptor,j,i Como Entero
	Definir DNIreceptor como cadena
	
	Confir_Dinero<-0
	
	Repetir
		Repetir
			cartel4(Dinero,ID)
			Escribir '                              'Sin Saltar
			Escribir '-Ingrese el DNI de la Persona de Destino:'
			Escribir '                              'Sin Saltar
			Leer DNIreceptor
			cont_trans<-0
			
			Para j<-0 Hasta 98 Con Paso 1 Hacer
				Si Matriz[j,3]==DNIreceptor Entonces
					cont_trans<-cont_trans+1
					ID_Receptor<-j
				Fin Si
			Fin Para
			
			Si cont_trans==1 Entonces
				Escribir '                              'Sin Saltar
				Escribir '-¿Cuánto Dinero desea Transferir?:'
				Repetir
					Escribir '                              'Sin Saltar
					Leer Trans_Dinero
					Si Trans_Dinero>Dinero[ID] Entonces
						Escribir '                              'Sin Saltar
						Escribir '-¡Saldo Insuficiente!'
					Fin Si
				Hasta Que Trans_Dinero>0 y Trans_Dinero<=Dinero[ID]
				Escribir ''
				Escribir '                              'Sin Saltar
				Escribir '1-Confirmar.'
				Escribir '                              'Sin Saltar
				Escribir '2-Reingresar.'
				Repetir
					Escribir '                              'Sin Saltar
					Leer Confir_Dinero
				Hasta Que Confir_Dinero==1 o Confir_Dinero==2
				
				Dinero[ID]<-Dinero[ID]-Trans_Dinero
				Dinero[ID_Receptor]<-Dinero[ID_Receptor]+Trans_Dinero
				
				//Hacemos el historial, que detecta si esta en 0. Para el receptor va positivo y el emisor va negativo.
				Para i<-0 Hasta 19 Con Paso 1 Hacer
					Escribir "ID_Receptor-> " ID_Receptor
					Si Historial[ID_Receptor,i] = 0 Entonces
						Historial[ID_Receptor,i] <- Trans_Dinero
						i<-19
					Fin Si
				Fin Para 
				
				Para j<-0 Hasta 19 Con Paso 1 Hacer
					Escribir "ID -> " ID
					Si Historial[ID,j] = 0 Entonces
						Historial[ID,j] <- (Trans_Dinero * (-1))
						j <-19
					FinSi
				Fin Para 
			SiNo
				Escribir '                              'Sin Saltar
				Escribir '-¡El Usuario NO Existe!'
				Esperar 5 Segundos
			Fin Si
			
		Hasta Que Confir_Dinero == 1
		
		cartel4(Dinero,ID)
		Escribir '                              'Sin Saltar
		Escribir '1-Seguir Transfiriendo.'
		Escribir '                              'Sin Saltar
		Escribir '2-Menu Principal.'
		
		Repetir
