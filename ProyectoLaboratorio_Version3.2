							Matriz[ID_DESBLOQUEAR,6] <- "0"
							ESTADO <- "Bloqueado"
							Escribir "El usuario " Matriz[ID_DESBLOQUEAR,1] " " Matriz[ID_DESBLOQUEAR,2] " ahora esta " ESTADO
						Fin Si
					SiNo
						Si Matriz[ID_DESBLOQUEAR,6] == "0" Entonces
							ESTADO <- "Bloqueado"
							Escribir "El usuario " Matriz[ID_DESBLOQUEAR,1] " " Matriz[ID_DESBLOQUEAR,2] " actualmente se encuentra " ESTADO ", Desea cambiar su estado?"
							Escribir "1- SI"
							Escribir "2- NO"
							LEER CAMBIO_ESTADO
